{
  "name": "pr-auto-review",
  "version": "1.0.0",
  "description": "Automated pull request review tool with ESLint checks, Jest tests, and GitHub API integration.",
  "main": "index.js",
  "bin": {
    "pr-auto-review": "./scripts/pr-check.js"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.mjs,.cjs",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --coverageReporters=json-summary",
    "generate-event": "node ./scripts/generate-event.js",
    "pr-check": "node ./scripts/pr-check.js",
    "pr-test": "npm run generate-event && npm run pr-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyawsoe-dev/nestjs-generator-cli.git"
  },
  "keywords": [
    "github",
    "pull-request",
    "pr-review",
    "eslint",
    "jest",
    "automation",
    "ci-cd",
    "code-quality",
    "octokit"
  ],
  "author": "Kyaw Soe",
  "license": "MIT",
  "homepage": "https://github.com/kyawsoe-dev/nestjs-generator-cli#readme",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "dotenv": "^17.2.2"
  }
}
